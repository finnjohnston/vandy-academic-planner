                  "exclude": [
                    "BME 2860",
                    "CHBE 2860",
                    "CE 2860",
                    "DF 2860",
                    "DS 2860",
                    "ECE 2860",
                    "ENGM 2860",
                    "ENVE 2860",
                    "ES 2860",
                    "ME 2860",
                    "MSE 2860",
                    "NANO 2860",
                    "SC 2860",
