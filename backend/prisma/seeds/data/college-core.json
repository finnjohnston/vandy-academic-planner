{
  "sections": [
    {
        "id": "core_capacity_tagged_courses",
        "title": "Core Capacity Tagged Courses",
        "creditsRequired": 29,
        "requirements": [
        {
            "id": "first_year_core",
            "title": "First-Year Core",
            "description": "Take the following courses",
            "creditsRequired": 8,
            "rule": {
                "type": "take_courses",
                "courses": ["CORE 1010", "CORE 1020"]
            }
        },
        {
            "id": "exploratory_core",
            "title": "Exploratory Core",
            "description": "2 interdisciplinary, problem-oriented CORE 2500 courses, each fulfilling 2 Core Capacities",
            "creditsRequired": 6,
            "rule": {
                "type": "take_any_courses",
                "credits": 6,
                "filter": {
                    "type": "subject_number",
                    "subjects": ["CORE"],
                    "numbers": [{ "type": "specific", "values": ["2500"] }]
                }
            }
        },
        {
            "id": "humanities",
            "title": "Core Capacity Tagged Course: Humanities",
            "description": "One Humanities course with LE: HFA and at least one capacity attribute (A, B, C, D, or E)",
            "creditsRequired": 3,
            "rule": {
                "type": "take_any_courses",
                "credits": 3,
                "filter": {
                    "type": "composite",
                    "operator": "AND",
                    "filters": [
                    {
                        "type": "attribute",
                        "attributeType": "core",
                        "attributes": ["LE: HFA-Humanities and Fine Arts"]
                    },
                    {
                        "type": "attribute",
                        "attributeType": "core",
                        "attributes": [
                            "CORE: A-Written & Creative Expression",
                            "CORE: B-Systemic & Structural Reasoning",
                            "CORE: C-Cultural & Interpretive Investigation",
                            "CORE: D-Data Literacy & Computational Thinking",
                            "CORE: E-Ethical & Social Engagement"
                        ]
                    }
                    ]
                }
            }
        },
        {
            "id": "social_science",
            "title": "Core Capacity Tagged Course: Social Science",
            "description": "One Social Science course with LE: SBS attribute AND at least one capacity tag (A, B, C, D, or E)",
            "creditsRequired": 3,
            "rule": {
            "type": "take_any_courses",
            "credits": 3,
            "filter": {
                "type": "composite",
                "operator": "AND",
                "filters": [
                {
                    "type": "attribute",
                    "attributeType": "core",
                    "attributes": ["LE: SBS-Social and Behavioral Sciences"]
                },
                {
                    "type": "attribute",
                    "attributeType": "core",
                    "attributes": [
                        "CORE: A-Written & Creative Expression",
                        "CORE: B-Systemic & Structural Reasoning",
                        "CORE: C-Cultural & Interpretive Investigation",
                        "CORE: D-Data Literacy & Computational Thinking",
                        "CORE: E-Ethical & Social Engagement"
                    ]
                }
                ]
            }
            }
        },
        {
            "id": "natural_science",
            "title": "Core Capacity Tagged Course: Natural Science",
            "description": "One Natural Science course with LE: MNS attribute AND at least one capacity tag (A, B, C, D, or E)",
            "creditsRequired": 3,
            "rule": {
            "type": "take_any_courses",
            "credits": 3,
            "filter": {
                "type": "composite",
                "operator": "AND",
                "filters": [
                {
                    "type": "attribute",
                    "attributeType": "core",
                    "attributes": ["LE: MNS-Math and Natural Sciences"]
                },
                {
                    "type": "attribute",
                    "attributeType": "core",
                    "attributes": [
                        "CORE: A-Written & Creative Expression",
                        "CORE: B-Systemic & Structural Reasoning",
                        "CORE: C-Cultural & Interpretive Investigation",
                        "CORE: D-Data Literacy & Computational Thinking",
                        "CORE: E-Ethical & Social Engagement"
                    ]
                }
                ]
            }
            }
        },
        {
            "id": "any_tagged",
            "title": "Core Capacity Tagged Course: Any",
            "description": "Two additional courses tagged with 1-2 Core Capacities",
            "creditsRequired": 6,
            "rule": {
                "type": "take_any_courses",
                "credits": 6,
                "filter": {
                    "type": "attribute",
                    "attributeType": "core",
                    "attributes": [
                        "CORE: A-Written & Creative Expression",
                        "CORE: B-Systemic & Structural Reasoning",
                        "CORE: C-Cultural & Interpretive Investigation",
                        "CORE: D-Data Literacy & Computational Thinking",
                        "CORE: E-Ethical & Social Engagement"
                    ]
                }
            }
        }
        ]
    },
